
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Responsive Admin Dashboard | Korsat X Parmaga</title>
    <!-- ======= Styles ====== -->
    <link rel="stylesheet" href="~/finance/css/styles.css">
</head>

<body>
    <!-- =============== Navigation ================ -->
    <div class="container">
        <div class="navigation">
            <ul>
                <li>
                    <a asp-controller="Finance" asp-action="FinanceDashboard">
                        <span class="icon">
                            <ion-icon name="cash-outline"></ion-icon>
                        </span>
                        <span class="title">Finance Department</span>
                    </a>
                </li>

                <li>
                    <a asp-controller="Finance" asp-action="FinanceDashboard">
                        <span class="icon">
                            <ion-icon name="home-outline"></ion-icon>
                        </span>
                        <span class="title">Dashboard</span>
                    </a>
                </li>
                <li>
                    <a asp-controller="Customer" asp-action="ListCustomer">
                        <span class="icon">
                            <ion-icon name="people-outline"></ion-icon>
                        </span>
                        <span class="title">Customer</span>
                    </a>
                </li>

                <li>
                    <a asp-controller="Invoice" asp-action="Invoices">
                        <span class="icon">
                            <ion-icon name="receipt-outline"></ion-icon>
                        </span>
                        <span class="title">Invoices</span>
                    </a>
                </li>
                <li>
                    <a asp-controller="Customer" asp-action="FeedBack">
                        <span class="icon">
                            <ion-icon name="chatbubble-outline"></ion-icon>
                        </span>
                        <span class="title">FeedBack</span>
                    </a>
                </li>

                <li>
                    <a asp-controller="FinanceReport" asp-action="Index">
                        <span class="icon">
                            <ion-icon name="document-text-outline"></ion-icon>
                        </span>
                        <span class="title">FinanceReport</span>
                    </a>

                </li>
            

                <li>
                    <a asp-controller="finance" asp-action="login">
                        <span class="icon">
                            <ion-icon name="log-out-outline"></ion-icon>
                        </span>
                        <span class="title">Sign Out</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>

    <!-- ========================= Main ==================== -->
    <div class="main">
        <div class="topbar">
            <div class="toggle">
                <ion-icon name="menu-outline"></ion-icon>
            </div>

            <a>
                <div class="user">
                    <img src="~/finance/img/customer01.jpg" alt="">
                </div>
            </a>

            
        </div>

        <!-- ======================= Cards ================== -->
       

        <!-- ================ Order Details List ================= -->
      
            @RenderBody() <!-- Render the content of the view here -->
       
    </div>

    <!-- =========== Scripts =========  -->
    <script src="~/finance/js/mains.js"></script>

    <!-- ====== ionicons ======= -->
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
</body>

</html>
